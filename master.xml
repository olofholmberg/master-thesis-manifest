<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is selected unless other manifest is selected with -m at repo init. -->
<!-- Should probably use strict revisions in other tags than default to ensure compatibility -->
<!-- Master revision is probably fine for development remote and project though -->
<!-- To find the correct revision should probably check each seL4 repo for the latest green checkmark revision -->
<manifest>

    <!-- Private remote with the application code -->
    <remote name="private_gitlab" fetch="https://github.com/olofholmberg/" revision="master"/>
    
    <!-- The seL4 microkernel and related remotes -->
    <remote name="seL4" fetch="https://github.com/seL4/"/>
    <!-- Related projects to seL4 -->
    <remote name="sel4proj" fetch="https://github.com/seL4/sel4proj"/>
    
    <!-- The ZeroMQ remote (only for vm-examples) -->
    <remote name="zeromq" fetch="https://github.com/zeromq"/>
    
    <!-- The lwIP remote (only for vm-examples) -->
    <remote name="lwip" fetch="https://git.savannah.gnu.org/git/"/>
    
    <!-- The picoTCP remote (only for vm-examples) -->
    <remote name="picotcp" fetch="https://github.com/tass-belgium"/>
    
    <!-- The polly remote (only for vm-examples) -->
    <remote name="polly" fetch="https://github.com/ruslo"/>

    <!-- Default remote for the projects -->
    <default remote="seL4" revision="master"/>
    
    <!-- Code for the seL4 microkernel -->
    <project name="seL4.git" remote="seL4" path="kernel" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Collection of libraries for working on seL4 -->
    <project name="seL4_libs.git" remote="seL4" path="projects/seL4_libs" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Collections of libraries for seL4 that are compatible with seL4_libs -->
    <project name="seL4_projects_libs.git" remote="seL4" path="projects/seL4_projects_libs" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Build tools for seL4 projects -->
    <project name="seL4_tools.git" remote="seL4" path="tools/seL4" revision="master" upstream="master" dest-branch="master">
        <!-- File linking required to make sure build scripts go to the right location -->
        <linkfile src="cmake-tool/griddle" dest="griddle"/>
        <linkfile src="cmake-tool/init-build.sh" dest="init-build.sh"/>
    </project>
    
    <!-- Simple runtime for C or C-compatible processes in a minimal seL4 environment -->
    <project name="sel4runtime.git" remote="seL4" path="projects/sel4runtime" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Core of the CAmkES component platform -->
    <project name="camkes-tool.git" remote="seL4" path="projects/camkes-tool" revision="master" upstream="master" dest-branch="master">
        <linkfile src="docs/index.md" dest="camkes_README.md"/>
    </project>
    
    <!-- CAmkES VMM components, templates and interfaces -->
    <project name="camkes-vm.git" remote="seL4" path="projects/camkes-vm" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- CAmkES tools, CMake helpers, linux images and root filesystem artifacts -->
    <project name="camkes-vm-linux.git" remote="seL4" path="projects/camkes-vm-linux" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- CAmkES VM examples apps -->
    <project name="camkes-vm-examples.git" path="projects/vm-examples" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Binary images for CAmkES VMs -->
    <project name="camkes-vm-images.git" remote="seL4" path="projects/camkes-vm-images" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Reusable CAmkES components and interfaces -->
    <project name="global-components.git" remote="seL4" path="projects/global-components" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- Tools for generating, parsing and loading capdl specs of systems -->
    <project name="capdl.git" remote="seL4" path="projects/capdl" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- MIT-licensed implementation of the C standard lib targetting linux syscall API -->
    <project name="musllibc.git" remote="seL4" path="projects/musllibc" revision="sel4" upstream="sel4" dest-branch="sel4"/>

    <!-- Platform independent libraries -->
    <project name="projects_libs.git" remote="seL4" path="projects/projects_libs" revision="master" upstream="master" dest-branch="master"/>

    <!-- OS independent utility libs -->
    <project name="util_libs.git" remote="seL4" path="projects/util_libs" revision="master" upstream="master" dest-branch="master"/>
    
    <!-- ZeroMQ lightweight messaging kernel -->
    <project name="libzmq" path="projects/libzmq" remote="zeromq" revision="master" upstream="refs/tags/v4.2.5" dest-branch="refs/tags/v4.2.5"/>
    
    <!-- lwIP Lightweight TCP/IP stack -->
    <project name="lwip.git" path="projects/lwip" remote="lwip" revision="master" upstream="refs/tags/STABLE-2_1_2_RELEASE" dest-branch="refs/tags/STABLE-2_1_2_RELEASE"/>
    
    <!-- picoTCP TCP/IP stack for embedded systems -->
    <project name="picotcp.git" path="projects/picotcp" remote="picotcp" revision="master" upstream="refs/tags/v1.7.0" dest-branch="refs/tags/v1.7.0"/>

    <!-- polly CMake toolchain files and scripts -->
    <project name="polly" path="tools/polly" remote="polly" revision="master" upstream="master" dest-branch="master"/>

    <!-- Private project with the application code -->
    <project name="master-thesis-sel4-implementation.git" path="projects/master-thesis-implementation" remote="private_gitlab"  revision="master">
        <!-- Project specific easy settings (required for init-build.sh to work) -->
        <linkfile src="easy-settings.cmake" dest="easy-settings.cmake"/>
    </project>
    
    <!-- Private project with the yocto VM images and build -->
    <project name="master-thesis-sel4-images.git" path="projects/master-thesis-images" remote="private_gitlab" revision="master"/>
    
</manifest>
